{
  "name": "rentit",
  "version": "1.0.0",
  "description": "A community rental platform",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "NODE_ENV=test mocha --require dotenv/config --recursive --exit \"test/**/*.test.js\"",
    "coverage": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules c8 mocha --require dotenv/config --recursive --exit \"test/**/*.test.js\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "bcryptjs": "^2.4.3",
    "browser-image-compression": "^2.0.2",
    "cloudinary": "^2.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.8.7",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
    "c8": "^8.0.0",
    "chai": "^5.2.0",
    "jsdom": "^26.1.0",
    "mocha": "^11.2.2",
    "nodemon": "^3.1.9",
    "sinon": "^20.0.0",
    "supertest": "^7.1.0"
  },
  "c8": {
    "include": [
      "src/**/*.js",
      "public/**/*.js"
    ],
    "exclude": [
      "test/**",
      "src/scripts/**",
      "coverage/**"
    ],
    "all": true,
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "check-coverage": true,
    "branches": 70,
    "lines": 80,
    "functions": 80,
    "statements": 80
  }
}
